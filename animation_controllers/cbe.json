{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.cbe": {
			"initial_state": "cbe",
			"states": {
				"cbe": {
					"transitions": [{
						"beehive": "query.get_equipped_item_name == 'beehive'"
					}]
				},
				"beehive": {
					"on_entry": [
						"/clear @s[tag=!op,m=!c,tag=!bypass] beehive",
						"/scoreboard players add @s[tag=!op,m=!c,tag=!bypass] illegalitemsvl 1",
						"/execute @s[tag=!op,m=!c,tag=!bypass] ~~~ tellraw @a[tag=notify] {\"rawtext\":[{\"text\":\"§r§6[§aScythe§6]§r \"},{\"selector\":\"@s\"},{\"text\":\" §1has failed §7(Exploit) §4Command Block Exploit §7(beehive)§4. VL= \"},{\"score\":{\"name\":\"@s\",\"objective\":\"cbevl\"}}]}"
					],
					"transitions": [{
						"cbe": "query.get_equipped_item_name != 'beehive'"
					}],
					"on_exit": [
						""
					]
				}
			}
		}
	}
}
